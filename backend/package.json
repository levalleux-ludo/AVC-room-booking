        "start:env-preprod": "node  --require dotenv/config ./server.js dotenv_config_path=./env/.env.preprod",
        "test-debug": "MONGODB_URI='mongodb://localhost/AVC-room-booking-system-test' mocha --inspect-brk",
        "deploy:now": "now -n avc-room-booking",
